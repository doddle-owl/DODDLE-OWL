<?xml version="1.0" encoding="utf-8"?>

<jnlp spec="1.0+" codebase="http://doddle-owl.sourceforge.net/" href="${outputFile}">
   <information>
     <title>DODDLE</title>
     <vendor>DODDLE Project, Yamaguchi Laboratory, Keio University, Japan</vendor>
     <homepage href="http://doddle-owl.sourceforge.net/"/>
     <description>Domain Ontology rapiD Development Environment</description>
     <description kind="short">DODDLE</description>
	 <icon href="lib/doddle_logo.png"/>
     <offline-allowed/>
   </information>
   <security>
       <all-permissions/>
   </security>
   <resources>
    <j2se version="1.6+" java-vm-args="-Xmx512m -Xms256m"/>
